
{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "types": ["node", "cors"],
    "typeRoots": ["./node_modules/@types", "./src/domain/entities/types"]
    
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
